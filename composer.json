{
    "name": "honda/url-pattern-matcher",
    "description": "url-pattern-matcher",
    "keywords": [
        "honda",
        "url-pattern-matcher"
    ],
    "homepage": "https://github.com/laravel-honda/url-pattern-matcher",
    "license": "MIT",
    "authors": [
        {
            "name": "FÃ©lix Dorn",
            "email": "github@felixdorn.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.0.5",
        "friendsofphp/php-cs-fixer": "^3",
        "phpstan/phpstan": "^1.1",
        "symfony/var-dumper": "^5.2.0"
    },
    "autoload": {
        "psr-4": {
            "Honda\\UrlPatternMatcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Honda\\UrlPatternMatcher\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "lint": "php-cs-fixer fix -v",
        "test:lint": "php-cs-fixer fix -v --dry-run",
        "test:types": "phpstan analyse --ansi --memory-limit=-1",
        "test:unit": "pest --colors=always",
        "test": [
            "@test:lint",
            "@test:types",
            "@test:unit"
        ]
    }
}
